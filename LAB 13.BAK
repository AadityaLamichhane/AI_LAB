% By SriJan Karki

fib :-
    write('Enter the position: '), read(N),
    fibonacci(N, F),
    write('Fibonacci number at position '), write(N),
    write(' is '), write(F), nl.

fibonacci(0, 0).
fibonacci(1, 1).
fibonacci(N, F) :-
    N > 1,
    N1 is N - 1,
    N2 is N - 2,
    fibonacci(N1, F1),
    fibonacci(N2, F2),
    F is F1 + F2.

